<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/export_nav_graph"
    app:startDestination="@id/exportFrequencyFragment">

    <fragment
        android:id="@+id/exportFrequencyFragment"
        android:name="com.android.healthconnect.controller.export.ExportFrequencyFragment">
        <action
            android:id="@+id/action_exportFrequencyFragment_to_exportEncryptionFragment"
            app:destination="@id/exportEncryptionFragment" />
    </fragment>

    <fragment
        android:id="@+id/exportEncryptionFragment"
        android:name="com.android.healthconnect.controller.export.ExportEncryptionFragment">
        <action
            android:id="@+id/action_exportEncryptionFragment_to_exportFrequencyFragment"
            app:destination="@id/exportFrequencyFragment" />
    </fragment>
</navigation>
